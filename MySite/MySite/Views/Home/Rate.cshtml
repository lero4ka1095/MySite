@model IEnumerable<DataGetLibrary.Rate>

<h2>Курс валют за @ViewBag.Date</h2>

<table class="table">
    <tr>
        <th>
            Валюта
        </th>
        <th>
            Курс
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Currency.CharCodeCurrency)
            </td>
            <td>
                @item.getValue()
            </td>
        </tr>
    }
</table>
